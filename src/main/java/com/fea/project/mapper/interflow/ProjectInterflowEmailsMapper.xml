    <select id="selectModelVoById" parameterType="long" resultMap="BaseResultMapVo">
        select
        tpie.*,
        tpp.name as projectName,
        tps.name as projectStepName,
        tptc.task_name
        from t_project_interflow_emails as tpie
        left join t_project_project as tpp on tpie.project_id=tpp.id
        left join t_project_step as tps on tpie.project_step=tps.id
        left join t_project_task_create as tptc on tpie.task_id=tptc.task_id
        where tpie.id = #{id,jdbcType=BIGINT}
    </select>
